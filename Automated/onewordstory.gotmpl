{{/*
	Trigger Type: Regex
	Trigger: \A
	
	Made by Black Wolf#0001
*/}}

{{$user := 0}}
{{with (dbGet 2000 "onewordstory")}}
	{{$user = toInt .Value}}
{{end}}

{{if eq .User.ID $user}}
	{{deleteTrigger 1}}
{{else}}
	{{if reFind `\A\S+\z` .Message.Content}}
		{{dbSet 2000 "onewordstory" (str .User.ID)}}
	{{else}}
		{{deleteTrigger 1}}
	{{end}}
{{end}}