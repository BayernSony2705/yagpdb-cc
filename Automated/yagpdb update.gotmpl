{{/*
	Trigger: 10 Minutes
	Trigger Type: Interval

	Note: If if the command runs for the first time it will output a preview / example on how it will look.
	That means if it ran once it will work afterwards.

	Need help or you found a bug? Join https://discord.gg/GRns3fg or report it on GitHub.

	Copyright: 2020 TheHDCrafter
	License: MIT
	Repository: https://github.com/TheHDCrafter/yagpdb-cc/
*/}}
{{$utcoffset := 1}}
{{/*Input your UTC time zone. Example is Germany with +1 hour*/}}
{{$channel := .Channel.ID}}
{{/*The channel it should be sending the message in, .Channel.ID = current channel*/}}

{{$version := index (reFindAllSubmatches `YAGPDB Status, version (.+)` (index (exec "yagstatus")).Title) 0 1}}
{{$yagversion := sdict}}{{with dbGet 204255221017214977 "yagversion"}}{{$yagversion = sdict .Value}}{{end}}
{{$firsttime := ""}}
{{if not $yagversion.version}}
	{{$yagversion = sdict "version" "No old versions recorded" "last_update" ((newDate 2015 1 1 0 0 0).Add ((currentTime.Sub ((newDate 2015 1 1 0 0 0).Add (toDuration (mult 5329 .TimeHour))))))}}{{$firsttime = " - ‚ùóExample‚ùó "}}
{{end}}
{{if not (eq $yagversion.version $version)}}
	{{$days := fdiv (currentTime.Sub $yagversion.last_update).Hours 24}}
	{{$y := fdiv $days 365.25|roundFloor|toFloat}}
	{{$mo := (sub (fdiv $days 30.417) (mult $y 12))|roundFloor|toFloat}}
	{{$d := toFloat (sub $days (add (mult $y 365.25) (mult $mo 30.417)))}}
	{{sendMessage $channel (cembed
		"fields" (cslice
			(sdict "name" "**‚Ä¢ Version:** <:editmessage:710443113902243851>" "value" (print "Old: \u2004\u200A`" $yagversion.version "`\nNew: `" $version "`") "inline" true)
			(sdict "name" "**‚Ä¢ Time:** <:stopwatch1:650328464825516062>" "value" (print "Time that passed since the last update:\n**" (humanizeDurationHours (currentTime.Sub $yagversion.last_update)) "**\n" ($yagversion.last_update.UTC.Format "üìÖ**02.01.06 ‚åö15:04**") "\n‚åõ\u200A**" (toInt $y) "Y " (toInt $mo) "M " (toInt $d) "D**\n") "inline" true))
		"author" (sdict "icon_url" "https://cdn.discordapp.com/emojis/708214342067421215.png" "name" (print "YAGPDB updated" $firsttime))
		"color" 0x738BD7
		"footer" (sdict "text" "Current time")
		"timestamp" currentTime)}}
	{{dbSet 204255221017214977 "yagversion" (sdict "version" $version "last_update" (currentTime.Add (toDuration (mult $utcoffset .TimeHour))))}}
{{end}}